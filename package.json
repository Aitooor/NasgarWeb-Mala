{
  "name": "nasgar-web",
  "version": "1.0.0",
  "description": "",
  "main": "bin/www",
  "bin": {
    "nasgar-web": "bin/www"
  },
  "type": "commonjs",
  "module": "nasgar-web",
  "typings": "@types",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "node bin/www",
    "start_forever": "( SET NODE_ENV=production || NODE_ENV=production ) && forever start -a -c \"node --no-warnings\" --uid \"web\" bin/www",
    "dev": "node ./node_modules/nodemon/bin/nodemon.js bin/www",
    "dev:ts": "tsc -p tsconfig.json",
    "dev:ts-public": "tsc -p tsconfig-public.json",
    "ts": "yarn run dev:ts",
    "tsp": "yarn run dev:ts-public",
    "commit": "git add . && git commit -m \"$1\"",
    "hub": "git push origin master && git push nasgar master",
    "compile_less_common": "lessc ./src/public/less/common/super.less ./src/public/css/common.css -x --source-map",
    "compile_less_components": "lessc ./src/public/less/components/select.less ./src/public/css/components/select.css -x --source-map",
    "compile_less_partials": "cd src/public && lessc ./less/partials/partials.less ./css/partials/partials.css -x --source-map --source-map-url=/css/partials/partials.css.map",
    "compile_less": "npm run compile_less_common && npm run compile_less_components && npm run compile_less_partials"
  },
  "keywords": [],
  "author": "Nasgar Network",
  "license": "ISC",
  "dependencies": {
    "cloudinary": "^1.27.1",
    "cookie-parser": "^1.4.5",
    "crypto": "^1.0.1",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-session": "^1.17.2",
    "hjson": "^3.2.2",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "node-fetch": "^3.0.0",
    "nodemailer": "^6.6.5",
    "paypal-rest-sdk": "^1.8.1",
    "rcon": "git+https://github.com/tehbeard/node-rcon.git",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.1.7",
    "@types/express-session": "^1.17.4",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mysql": "^2.15.19",
    "@types/nodemailer": "^6.4.4",
    "@types/paypal-rest-sdk": "^1.7.6",
    "@types/simplemde": "^1.11.8",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@types/uuid": "^8.3.1",
    "@types/winston": "^2.4.4",
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.7",
    "typescript": "^4.4.4"
  }
}
