<% const _url = typeof url === "string" ? url : "/index" %>
<% const title_inner = typeof title === "string" ? title : _url %>
<% const less_paths = typeof styles === "object" ? styles : [ "pags" + _url ]; %> 
<% const js_paths   = typeof scripts === "object" ? scripts : [ "pg" + _url ]; %> 

<!DOCTYPE html>
<html lang="en">
<head>
  <%- include("./header.ejs") %> 

  <title><%= title_inner %> - Nasgar Network</title>

  <meta rel="canonical" href="<%- global.const.WEB_HREF %><%- _url %>">

  <%- include("./less.ejs", { paths: less_paths }) %> 
  <%- include("./script.ejs", { paths: js_paths }) %> 
