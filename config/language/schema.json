{
  "$schema": "http://json-schema.org/schema",
  "$id": "Language Nasgar Json",
  "required": ["name"],
  "properties": {
    "name": {
      "type": "object",
      "properties": {
        "native": {
          "type": "string"
        },
        "english": {
          "type": "string"
        }
      }
    },
    "menu": {
      "type": "object",
      "properties": {
        "home": {
          "type": "string",
          "default": "Home"
        },
        "updates": {
          "type": "string",
          "default": "Updates"
        },
        "vote": {
          "type": "string",
          "default": "Vote"
        },
        "bans": {
          "type": "string",
          "default": "Bans"
        },
        "shop": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string",
              "default": "Shop"
            },
            "Ranks": {
              "type": "string",
              "default": "Ranks"
            },
            "Keys": {
              "type": "string",
              "default": "Keys"
            }
          }
        },
        "login": {
          "type": "string",
          "default": "Login"
        }
      }
    },

    "landing": {
      "type": "object",
      "default": {},
      "properties": {
        "description": {
          "type": "string",
          "default": "Play <b>Nasgar</b> today."
        },

        "about": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string",
              "default": "About us"
            },
            "content": {
              "type": "string",
              "default": ""
            }
          }
        }
      }
    },

    "footer": {
      "type": "string",
      "default": "<span>This server is <b>not</b> associated with <b>Mojang, Minecraft or Microsoft</b> this website isn't associated with this companys and is owner by <b>Nasgar Network</b>.</span><br><span>Â© Copyright 2021 <b>Nasgar Network</b>.</span>"
    }
  }
}
